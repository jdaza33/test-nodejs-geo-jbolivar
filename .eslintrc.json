{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": 0,
        "spaced-comment": 0,
        "object-curly-spacing": 0,
        "indent": 0,
        "no-trailing-spaces": 0,
        "comma-dangle": 0,
        "curly": 0,
        "prefer-promise-reject-errors": 0,
        "new-cap": 0,
        "valid-jsdoc": 0
    }
}